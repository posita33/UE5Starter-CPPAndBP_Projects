*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
SourceArt/**/*.png
SourceArt/**/*.tga

# Binary Files
CPP_BP/Binaries/*
CPP_BP/Plugins/*/Binaries/*

# Builds
CPP_BP/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!CPP_BP/Build/*/
CPP_BP/Build/*/**
!CPP_BP/Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!CPP_BP/Build/**/*.ico

# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
CPP_BP/Saved/*

# Compiled source files for the engine to use
CPP_BP/Intermediate/*
CPP_BP/Plugins/*/Intermediate/*

# Cache files for the editor to use
CPP_BP/DerivedDataCache/*
